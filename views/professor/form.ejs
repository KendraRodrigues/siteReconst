<html>
	<head>
		<title>Organizações Tabajara</title>
		<meta charset="UTF-8">
	</head>
	<body>
		<h1>Cadastro de Professor</h1>
		<hr>
		<form action="/atualizarProfessor" method="post">
				
			<label for="matricula">Matricula:</label><br>
			<input type="text" name="matricula" id="matricula" value="<%= professor[0].matricula %>" readonly="readonly" size="15"><br><br>
	
			<label for="senha">Senha:</label><br>
			<input type="text" name="senha" id="senha" value="<%= professor[0].senha %>" size="30"><br><br>

            <label for="nome">Nome:</label><br>
			<input type="text" name="nome" id="nome" value="<%= professor[0].nome %>" size="30"><br><br>
			
			<label for="telefone">Telefone:</label><br>
			<input type="text" name="telefone" id="telefone" value="<%= professor[0].telefone %>" size="20"><br><br>

            <label for="e_mail">Email:</label><br>
			<input type="text" name="e_mail" id="e_mail" value="<%= professor[0].e_mail %>" size="20"><br><br>
			
            <!-- INSERIR TITULAÇÕES -->

			<input type="submit" name="salvar" value="Salvar">
			<input type="reset" name="limpar" value="Limpar">
		</form>
	</body>
</html>